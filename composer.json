{
	"require": {
		"cakephp/cakephp": "*",
		"cakephp/debug_kit": "dev-master",
		"cakedc/search": "dev-master",
		"slywalker/debug_kit_plus": "dev-master",
		"slywalker/mysql_util": "dev-master",
		"slywalker/base": "dev-master",
		"ccampbell/chromephp": "dev-master"
	},
	"config": {
		"vendor-dir": "Vendor"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "cakephp/cakephp",
				"version": "2.3.5",
				"source": {
					"url": "git://github.com/cakephp/cakephp.git",
					"type": "git",
					"reference": "master"
				},
				"require": {
					"composer/installers": "*"
				}
			},
			"packagist": false
		},
		{
			"type": "package",
			"package": {
				"name": "cakedc/search",
				"type": "cakephp-plugin",
				"version": "dev-master",
				"source": {
					"url": "git://github.com/CakeDC/search.git",
					"type": "git",
					"reference": "master"
				},
				"require": {
					"composer/installers": "*"
				}
			},
			"packagist": false
		}
	]
}
